{
  "name": "koa-basic-server",
  "version": "1.0.0",
  "description": "Koa Basic Server",
  "main": "./server",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --debug ./server | ./node_modules/.bin/bunyan",
    "i-test": "NODE_ENV=test node ./node_modules/.bin/_mocha \"./**/*.i-spec.js\"",
    "test": "NODE_ENV=test node ./node_modules/.bin/_mocha \"./**/*.spec.js\"",
    "watch-i-test": "./node_modules/.bin/nodemon --exec 'npm run i-test'",
    "watch-test": "./node_modules/.bin/nodemon --exec 'npm test'"
  },
  "keywords": [
    "Koa",
    "API",
    "Server"
    "Restful",
    "Starter"
  ],
  "author": "Markus Daehn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/markusdaehn/koa-api-starter/issues"
  },
  "homepage": "https://github.com/markusdaehn/koa-api-starter#readme",
  "dependencies": {
    "bunyan": "^1.8.4",
    "co": "^4.6.0",
    "glob": "^7.1.1",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.2.0",
    "koa-bunyan": "^1.0.0",
    "koa-bunyan-logger": "^1.3.0",
    "koa-router": "^5.4.0",
    "ramda": "^0.22.1",
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.6"
  }
}
